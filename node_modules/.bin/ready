#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules/@bpinternal/readiness/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules/@bpinternal/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules/@bpinternal/readiness/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules/@bpinternal/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/@bpinternal+readiness@0.0.1/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bpinternal/readiness/bin.js" "$@"
else
  exec node  "$basedir/../@bpinternal/readiness/bin.js" "$@"
fi
