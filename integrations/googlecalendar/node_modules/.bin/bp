#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/test/Projects/Interesting/botpress/packages/cli/node_modules:/Users/test/Projects/Interesting/botpress/packages/node_modules:/Users/test/Projects/Interesting/botpress/node_modules:/Users/test/Projects/Interesting/node_modules:/Users/test/Projects/node_modules:/Users/test/node_modules:/Users/node_modules:/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/test/Projects/Interesting/botpress/packages/cli/node_modules:/Users/test/Projects/Interesting/botpress/packages/node_modules:/Users/test/Projects/Interesting/botpress/node_modules:/Users/test/Projects/Interesting/node_modules:/Users/test/Projects/node_modules:/Users/test/node_modules:/Users/node_modules:/node_modules:/Users/test/Projects/Interesting/botpress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@botpress/cli/bin.js" "$@"
else
  exec node  "$basedir/../@botpress/cli/bin.js" "$@"
fi
